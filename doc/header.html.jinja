<script type="text/javascript">
Prism.languages.jsonnet = {
  'cppcomment': /\/\/.*/g,
  'comment': /\/\*[\w\W]*?\*\//g,
  'string': /("|')(\\?.)*?\1/g,
  'keyword': /\b(self|super)\b|\$/g,
  'boolean': /\b(true|false)\b/g,
  'constant': /\bnull\b/g,
  'error': /\berror\b/g,
  'special': /\b(local|function|if|then|else|import|for|in)\b/g,
